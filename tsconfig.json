{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "outDir": "dist"
  }
}
